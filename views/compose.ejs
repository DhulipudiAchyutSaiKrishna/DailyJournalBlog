<%- include("partials/header");%>
<h1>Compose</h1>
<form action="/compose" method="post">
  <div class="form-group">
    <label for="composeTitle">Title</label> <br>
    <input class="form-control" type="text" name="composeTitle"><br>
    <label for="postData">Post</label><br>
    <textarea class="form-control" name="postData" rows="3"></textarea> <br>
  </div>

  <button class="btn btn-primary" type="submit" name="publishData">Publish</button>
</form>


